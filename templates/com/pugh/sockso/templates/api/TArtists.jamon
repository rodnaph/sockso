
<%import>
    com.pugh.sockso.music.Artist;
    com.pugh.sockso.JsonUtils;
    java.util.List;
</%import>

<%args>
    Artist[] artists = null;
</%args>

<%java>
    boolean isFirst = true;
</%java>

[
    <%for Artist artist : artists %>

        <%if !isFirst %>,</%if>
        <%java>isFirst = false;</%java>

        {
            "id": <% artist.getId() %>,
            "name": <% JsonUtils.string(artist.getName()) #n %>,
            "date_added": "<% artist.getDateAdded().toString() %>"
        }

    </%for>
]
